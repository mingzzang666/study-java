package arrayTest;

public class ArTask02 {
		public static void main(String[] args) {
//				다음과 같은 값이 주어졌을 때, 정수 배열의 합 구하기
			int[] arData = {10, 20, 30, 40, 50};
			int result = 0;
			
			for (int i = 0; i < arData.length; i++) {
					result = arData + arData[i];
			}
			
			System.out.println(result);
			
		}
}
